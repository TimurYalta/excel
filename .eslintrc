{
    "parser": "babel-eslint",
    "env":{
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4],
        "comma-dangle":["error", "never"],
        "require-jsdoc": "off"
    },
    "extends": ["eslint:recommended", "google"]
}